{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DFCovidKT"
		},
		"ls_Gen2_StorageAccount_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_Gen2_StorageAccount'"
		},
		"SA_ListOfFiles_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SA_ListOfFiles'"
		},
		"ls_Gen2_StorageAccount_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidsag2saprocessing.dfs.core.windows.net/"
		},
		"ls_Soure_Coviddata_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://github.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DFCovidKT_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_Gen2_StorageAccount_accountKey": {
						"value": "[parameters('ls_Gen2_StorageAccount_accountKey')]"
					},
					"SA_ListOfFiles_connectionString": {
						"value": "[parameters('SA_ListOfFiles_connectionString')]"
					},
					"ls_Gen2_StorageAccount_properties_typeProperties_url": {
						"value": "[parameters('ls_Gen2_StorageAccount_properties_typeProperties_url')]"
					},
					"ls_Soure_Coviddata_properties_typeProperties_url": {
						"value": "[parameters('ls_Soure_Coviddata_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}